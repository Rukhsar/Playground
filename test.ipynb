        "# prompt: print hello world using rot13\n",
        "\n",
        "import codecs\n",
        "\n",
        "def rot13(text):\n",
        "  return codecs.encode(text, 'rot_13')\n",
        "\n",
        "hello_world_rot13 = rot13(\"Hello, world!\")\n",
        "print(hello_world_rot13)\n"
